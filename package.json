{
  "name": "microsoft-intune",
  "version": "2.3.0",
  "description": "sdk for Microsoft Intune",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "run-s -l clean test tsc copy",
    "build:check": "tsc --noEmit",
    "clean": "rimraf coverage dist tmp",
    "commit": "cz",
    "copy": "pinst --disable && cpy package.json dist && cpy README.md dist && pinst --enable",
    "generate-docs": "typedoc",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "postinstall": "husky install",
    "qa": "run-s -l lint test clean build:check",
    "semantic-release": "semantic-release",
    "test": "jest",
    "tsc": "tsc -p tsconfig.build.json"
  },
  "keywords": [],
  "author": "Santese Smith",
  "repository": {
    "type": "git",
    "url": "https://github.com/santese/intune-node.git"
  },
  "license": "MIT",
  "dependencies": {
    "@azure/identity": "1.5.2",
    "@azure/storage-blob": "12.16.0",
    "@microsoft/microsoft-graph-client": "3.0.7",
    "@microsoft/microsoft-graph-types-beta": "0.28.0-preview",
    "isomorphic-fetch": "3.0.0",
    "tslib": "2.6.2"
  },
  "devDependencies": {
    "@digitalroute/cz-conventional-changelog-for-jira": "7.5.1",
    "@pliancy/eslint-config-ts": "0.0.5",
    "@pliancy/semantic-release-config-npm": "2.2.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.1.0",
    "@semantic-release/npm": "8.0.3",
    "@types/isomorphic-fetch": "0.0.36",
    "@types/jest": "27.5.2",
    "@types/node": "16.18.52",
    "@types/qs": "6.9.8",
    "commitizen": "4.3.0",
    "cpy-cli": "3.1.1",
    "husky": "7.0.4",
    "jest": "28.1.3",
    "npm-run-all": "4.1.5",
    "pinst": "2.1.6",
    "rimraf": "3.0.2",
    "semantic-release": "17.4.7",
    "ts-jest": "28.0.8",
    "ts-node": "9.1.1",
    "typedoc": "0.25.1",
    "typedoc-plugin-missing-exports": "0.23.0",
    "typescript": "4.9.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira",
      "skipScope": false,
      "jiraOptional": true
    }
  },
  "volta": {
    "node": "16.20.2"
  }
}
